                <label class="form-label" asp-for="Uygulama_Tip"></label>
                <input class="form-control" asp-for="Uygulama_Tip" />
            </div>
            <button type="submit" id="pay" class="btn btn-primary mt-4 w-100">Gönder</button>
        </section>
    </div>
    @Html.AntiForgeryToken()
</form>
