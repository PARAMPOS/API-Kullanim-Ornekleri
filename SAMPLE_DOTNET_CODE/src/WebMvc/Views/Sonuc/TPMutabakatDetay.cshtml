@using WebMvc.Models.TPMutabakatDetay;
@model TPMutabakatDetaySonucViewModel
@{
    ViewData["Title"] = "TPMutabakatDetay";
    ViewData["Menu"] = "7/1";
}
<div class="d-flex justify-content-center mt-2" style="width: 100%">
    <h3>Mutabakat Detay (TP_Mutabakat_Detay)</h3>
</div>
@if (Model.Sonuc is not null)
{
    <div class="pt-2 ps-2" style="width: calc(100% - 40px)">
        @{
            if (Model.Sonuc == "1")
            {
                <div class="alert alert-success" role="alert">
                    Sonuç Başarılı!
                </div>
            }
            else
            {
                <div class="alert alert-danger" role="alert">
                    İşlem Başarısız!
                </div>
            }
        }
    </div>
    <table class="table table-bordered ms-2 mt-2" style="width: calc(100% - 50px)">
        <thead>
            <tr>
                <th scope="col">Parametre</th>
                <th scope="col">Alt Parametre</th>
                <th scope="col">Değer</th>
            </tr>
        </thead>
        <tbody>
            @if (Model.DT_Bilgi != null)
            {
                <tr>
                    <th scope="row" rowspan="15">DT_Bilgi</th>
                    <th scope="row">GUNSONU_TARIHI</th>
                    <td>@Model.DT_Bilgi.GUNSONU_TARIHI</td>
                </tr>
                <tr>
                    <th scope="row">ISLEM_TARIHI</th>
                    <td>@Model.DT_Bilgi.ISLEM_TARIHI</td>
                </tr>
                <tr>
                    <th scope="row">VALOR_TARIHI</th>
                    <td>@Model.DT_Bilgi.VALOR_TARIHI</td>
                </tr>
                <tr>
                    <th scope="row">KART_NO</th>
                    <td>@Model.DT_Bilgi.KART_NO</td>
                </tr>
                <tr>
                    <th scope="row">TRANSACTION_TIPI</th>
                    <td>@Model.DT_Bilgi.TRANSACTION_TIPI</td>
                </tr>
                <tr>
                    <th scope="row">PROVIZYON_NO</th>
                    <td>@Model.DT_Bilgi.PROVIZYON_NO</td>
                </tr>
                <tr>
                    <th scope="row">TAKSIT_SIRA</th>
                    <td>@Model.DT_Bilgi.TAKSIT_SIRA</td>
                </tr>
                <tr>
                    <th scope="row">TAKSIT_SAYISI</th>
                    <td>@Model.DT_Bilgi.TAKSIT_SAYISI</td>
                </tr>
                <tr>
                    <th scope="row">PROVIZYON_TUTARI</th>
                    <td>@Model.DT_Bilgi.PROVIZYON_TUTARI</td>
                </tr>
                <tr>
                    <th scope="row">KOMISYON_TUTARI</th>
                    <td>@Model.DT_Bilgi.KOMISYON_TUTARI</td>
                </tr>
                <tr>
                    <th scope="row">KOMISYON_ORANI</th>
                    <td>@Model.DT_Bilgi.KOMISYON_ORANI</td>
                </tr>
                <tr>
                    <th scope="row">NET_TUTAR</th>
                    <td>@Model.DT_Bilgi.NET_TUTAR</td>
                </tr>
                <tr>
                    <th scope="row">SIPARIS_NO</th>
                    <td>@Model.DT_Bilgi.SIPARIS_NO</td>
                </tr>
                <tr>
                    <th scope="row">ANA_KART_TIPI</th>
                    <td>@Model.DT_Bilgi.ANA_KART_TIPI</td>
                </tr>
                <tr>
                    <th scope="row">ALT_KART_TIPI</th>
                    <td>@Model.DT_Bilgi.ALT_KART_TIPI</td>
                </tr>
            }
            <tr>
                <th scope="row" colspan="2">Sonuc</th>
                <td>@Model.Sonuc</td>
            </tr>
            <tr>
                <th scope="row" colspan="2">Sonuc_Str</th>
                <td>@Model.Sonuc_Str</td>
            </tr>
        </tbody>
    </table>
}
<br />